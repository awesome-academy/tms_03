<tr>
  <td><%= user_counter+1 %></td>
  <td><%= user.email %></td>
  <td><%= user.name %></td>
  <td><%= l user.created_at, format: :short %></td>
  <td><%= l user.updated_at, format: :short %></td>
  <td>
    <%= link_to "Delete", admin_user_path(user), method: :delete, data: {confirm: t(:are_you_sure)} %>
  </td>
</tr>
